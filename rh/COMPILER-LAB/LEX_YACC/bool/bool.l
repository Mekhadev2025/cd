%{
#include "y.tab.h"  // Include header generated by YACC
%}

%%

true       { return TRUE; }
false      { return FALSE; }
"&&"       { return AND; }
"||"       { return OR; }
"!"        { return NOT; }
"("        { return '('; }
")"        { return ')'; }
[ \t\n]+   { /* Ignore whitespace */ }
.          { yyerror("Invalid character"); }


%% 

int yywrap() {
    return 1;
}

